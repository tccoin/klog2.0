{
  "shell": "src/klog-app.html",
  "entrypoint": "index.html",
  "fragments": [
    "src/klog-timeline.html",
    "src/klog-article.html",
    "src/klog-editor.html",
    "src/klog-signup.html",
    "src/klog-login.html",
    "src/klog-userpanel.html",
    "src/klog-note.html",
    "src/klog-drive.html",
    "src/klog-zone.html",
    "src/klog-apps.html",
    "src/klog-apps-money.html",
    "src/klog-apps-kdoc.html",
    "src/klog-404.html",
    "src/klog-lab.html",
    "src/klog-render-katex.html"
  ],
  "npm": true,
  "moduleResolution": "node",
  "sources": [
    "src/**"
  ],
  "extraDependencies": [
    "bower_components/webcomponentsjs/webcomponents-loader.js",
    "node_modules/@webcomponents/webcomponentsjs/**/*",
    "node_modules/@webcomponents/html-imports/**/*",
    "bower_components/Han/dist/font/*",
    "static/**/*",
    "manifest.json",
    "static/favicon.ico"
  ],
  "lint": {
    "rules": [
      "polymer-2-hybrid"
    ]
  },
  "builds": [
    {
      "preset": "es5-bundled",
      "js": {
        "minify": true,
        "compile": "es5"
      },
      "bundle": {
        "inlineScripts": false
      },
      "addServiceWorker": true,
      "basePath": false
    },
    {
      "preset": "es6-unbundled",
      "js": {
        "minify": true
      },
      "addServiceWorker": true,
      "basePath": true
    }
  ]
}