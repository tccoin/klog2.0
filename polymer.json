{
  "entrypoint": "index.html",
  "shell": "src/klog-app.js",
  "fragments": [
    "src/klog-timeline.js",
    "src/klog-article.js",
    "src/klog-layout.js",
    "src/klog-icons.js",
    "src/klog-editor.js",
    "src/klog-signup.js",
    "src/klog-login.js",
    "src/klog-userpanel.js",
    "src/klog-note.js",
    "src/klog-drive.js",
    "src/klog-zone.js",
    "src/klog-apps.js",
    "src/klog-404.js",
    "src/klog-lab.js",
    "src/klog-render-katex.js",
    "src/klog-apps-money.js",
    "src/klog-apps-kdoc.js",
    "src/klog-apps-lib.js"
  ],
  "npm": true,
  "moduleResolution": "node",
  "sources": [
    "src/**"
  ],
  "extraDependencies": [
    "node_modules/@webcomponents/webcomponentsjs/**",
    "static/**/*",
    "manifest.json",
    "static/favicon.ico"
  ],
  "lint": {
    "rules": [
      "polymer-2-hybrid"
    ]
  },
  "builds": [
    {
      "preset": "es5-bundled",
      "bundle": {
        "inlineScripts": false
      },
      "addServiceWorker": true,
      "insertPrefetchLinks": false
    },
    {
      "preset": "es6-unbundled",
      "addServiceWorker": true,
      "insertPrefetchLinks": false,
      "addPushManifest": true
    }
  ]
}